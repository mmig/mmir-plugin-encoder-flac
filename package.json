{
  "name": "mmir-plugin-encoder-flac",
  "version": "0.1.0",
  "description": "\r\n      Cordova plugin for the MMIR framework that adds a JavaScript based FLAC encoder; \r\n      uses libflac.js (BSD and partially MIT license), see https://github.com/mmig/libflac.js\r\n    ",
  "cordova": {
    "id": "mmir-plugin-encoder-flac",
    "platforms": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmig/mmir-plugins-media.git#master:mmir-plugin-encoder-flac"
  },
  "keywords": [
    "cordova",
    "audio",
    "codec",
    "flac",
    "ecosystem:cordova"
  ],
  "engines": [
    {
      "name": "cordova",
      "version": ">=5.0"
    }
  ],
  "dependencies": {
	  "mmir-plugin-encoder-core": "../mmir-plugin-encoder-core",
	  "mmir-install-cordova-assets": "../mmir-install-cordova-assets"
  },
  "scripts": {
	  "postinstall": "mmir-install-cordova-assets",
	  "preuninstall": "mmir-uninstall-cordova-assets"
  },
  "author": "russa, DFKI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmig/mmir-plugins-media/issues"
  },
  "homepage": "https://github.com/mmig/mmir-plugins-media/tree/master%3Ammir-plugin-encoder-flac#readme"
}
